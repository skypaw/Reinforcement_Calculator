<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>

<?import javafx.collections.FXCollections?>

<BorderPane prefHeight="600" prefWidth="900" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml"
            fx:controller="pl.pawz.zelbet.GUI.Controller">

    <left>
        <VBox prefWidth="150">
            <padding>
                <Insets top="10" right="10" bottom="10" left="10"/>
            </padding>
            <Button text="Przekrój Prostokątny" prefWidth="130"/>
            <Button text="Przekrój Teowy" prefWidth="130"/>
        </VBox>
    </left>


    <center>
        <GridPane hgap="5" vgap="10">

            <GridPane hgap="10" vgap="5" GridPane.columnIndex="0" GridPane.rowIndex="0">
                <padding>
                    <Insets top="5" right="5" bottom="5" left="5"/>
                </padding>

                <Label text="Przekrój Prostokątny" GridPane.columnIndex="0" GridPane.rowIndex="0"
                       GridPane.columnSpan="2"/>
                <Label text="Stan Graniczny Nośności" GridPane.columnIndex="0" GridPane.rowIndex="1"
                       GridPane.columnSpan="2"/>


                <Label text="M_ed" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
                <TextField GridPane.columnIndex="1" GridPane.rowIndex="3" promptText="[kNm]" prefWidth="70"/>

                <Label text="V_ed" GridPane.columnIndex="2" GridPane.rowIndex="3"/>
                <TextField GridPane.columnIndex="3" GridPane.rowIndex="3" promptText="[kN]" prefWidth="70"/>

                <Label text="N_ed" GridPane.columnIndex="4" GridPane.rowIndex="3"/>
                <TextField GridPane.columnIndex="5" GridPane.rowIndex="3" promptText="[kN]" prefWidth="70"/>
            </GridPane>

            <Label text="Geometria przekroju i Materiały" GridPane.columnIndex="0" GridPane.rowIndex="1"/>

            <GridPane hgap="5" vgap="5" GridPane.columnIndex="0" GridPane.rowIndex="2">
                <padding>
                    <Insets top="5" right="5" bottom="5" left="5"/>
                </padding>

                <Label text="b" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
                <TextField GridPane.columnIndex="1" GridPane.rowIndex="0" promptText="[cm]" prefWidth="70"/>

                <Label text="h" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
                <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" promptText="[cm]" prefWidth="70"/>

                <Label text="Klasa Betonu" GridPane.columnIndex="2" GridPane.rowIndex="0"/>
                <ChoiceBox GridPane.columnIndex="3" GridPane.rowIndex="0">
                    <items>
                        <FXCollections fx:factory="observableArrayList">
                            <String fx:value="C30/37"/>
                            <String fx:value="C20/25"/>
                            <String fx:value="C25/30"/>
                        </FXCollections>
                    </items>
                </ChoiceBox>

                <Label text="f_yk" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
                <TextField GridPane.columnIndex="3" GridPane.rowIndex="1" promptText="[MPa]" prefWidth="70"/>
            </GridPane>

            <Label text="Zbrojenie podłużne" GridPane.columnIndex="0" GridPane.rowIndex="3"/>

            <GridPane hgap="10" vgap="5" GridPane.columnIndex="0" GridPane.rowIndex="4">
                <padding>
                    <Insets top="5" right="5" bottom="5" left="5"/>
                </padding>

                <Label text="A_s1" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
                <TextField GridPane.columnIndex="1" GridPane.rowIndex="0" promptText="[mm]" prefWidth="70"/>

                <Label text="A_s2" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
                <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" promptText="[mm]" prefWidth="70"/>

                <Label text="a_1" GridPane.columnIndex="2" GridPane.rowIndex="0"/>
                <TextField GridPane.columnIndex="3" GridPane.rowIndex="0" promptText="[mm]" prefWidth="70"/>

                <Label text="a_2" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
                <TextField GridPane.columnIndex="3" GridPane.rowIndex="1" promptText="[mm]" prefWidth="70"/>
            </GridPane>

            <Label text="Zbrojenie poprzeczne" GridPane.columnIndex="0" GridPane.rowIndex="5"/>

            <GridPane hgap="10" vgap="5" GridPane.columnIndex="0" GridPane.rowIndex="6" fx:id="gridPaneRods">
                <padding>
                    <Insets top="5" right="5" bottom="5" left="5"/>
                </padding>

                <Label text="V_ed,red" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
                <TextField GridPane.columnIndex="1" GridPane.rowIndex="0" promptText="[wartość]" prefWidth="40"/>

                <CheckBox text="Pręty odgięte" GridPane.columnIndex="2" GridPane.rowIndex="0" fx:id="checkBoxRods" onAction="#checkBoxRod"/>

                <Label text="ctg Theta" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
                <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" promptText="[wartość]" prefWidth="40"/>

                <Label text="Kąt Alpha" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
                <TextField GridPane.columnIndex="3" GridPane.rowIndex="1" promptText="[°]" prefWidth="40"/>

                <Label text="A_sw1" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
                <TextField GridPane.columnIndex="1" GridPane.rowIndex="2" promptText="[mm]" prefWidth="70"/>


                <Label text="n_sw1" GridPane.columnIndex="2" GridPane.rowIndex="2"/>
                <TextField GridPane.columnIndex="3" GridPane.rowIndex="2" promptText="[szt]" prefWidth="40"/>


            </GridPane>


            <Label text="Stan Graniczny Użytkowalności" GridPane.columnIndex="0" GridPane.rowIndex="7"/>


            <GridPane hgap="5" vgap="5" GridPane.columnIndex="0" GridPane.rowIndex="8">
                <padding>
                    <Insets top="5" right="5" bottom="5" left="5"/>
                </padding>


                <Label text="M_Ek" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
                <TextField GridPane.columnIndex="1" GridPane.rowIndex="0" promptText="[kNm]" prefWidth="70"/>

                <Label text="M_ek,lt" GridPane.columnIndex="2" GridPane.rowIndex="0"/>
                <TextField GridPane.columnIndex="3" GridPane.rowIndex="0" promptText="[kNm]" prefWidth="70"/>

                <Label text="Obciążenie" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
                <ChoiceBox GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <items>
                        <FXCollections fx:factory="observableArrayList">
                            <String fx:value="Obciążenie doraźne"/>
                            <String fx:value="Obciążenie długotrwałe"/>

                        </FXCollections>
                    </items>
                </ChoiceBox>


                <Label text="t0" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
                <TextField GridPane.columnIndex="1" GridPane.rowIndex="3" promptText="[dni]" prefWidth="70"/>

                <Label text="RH" GridPane.columnIndex="0" GridPane.rowIndex="4"/>
                <TextField GridPane.columnIndex="1" GridPane.rowIndex="4" promptText="[%]" prefWidth="70"/>

                <Label text="Klasa Cementu" GridPane.columnIndex="2" GridPane.rowIndex="3"/>
                <ChoiceBox GridPane.columnIndex="3" GridPane.rowIndex="3">
                    <items>
                        <FXCollections fx:factory="observableArrayList">
                            <String fx:value="Klasa S"/>
                            <String fx:value="Klasa N"/>
                            <String fx:value="Klasa R"/>

                        </FXCollections>
                    </items>
                </ChoiceBox>

                <Label text="Cnom" GridPane.columnIndex="2" GridPane.rowIndex="4"/>
                <TextField GridPane.columnIndex="3" GridPane.rowIndex="4" promptText="[mm]" prefWidth="70"/>
            </GridPane>

            <Label text="Dodatkowe opcje" GridPane.columnIndex="0" GridPane.rowIndex="9"/>


            <GridPane hgap="5" vgap="5" GridPane.columnIndex="0" GridPane.rowIndex="10">
                <padding>
                    <Insets top="5" right="5" bottom="5" left="5"/>
                </padding>

                <CheckBox text="Efekty II rzędu" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
                <CheckBox text="Diganostyka" GridPane.columnIndex="0" GridPane.rowIndex="1"/>

            </GridPane>

            <Label text="Obliczenia" GridPane.columnIndex="0" GridPane.rowIndex="13"/>

            <GridPane hgap="5" vgap="5" GridPane.columnIndex="0" GridPane.rowIndex="14" >
                <padding>
                    <Insets top="5" right="5" bottom="5" left="5"/>
                </padding>

                <CheckBox text="Zbrojenie podłużne" GridPane.columnIndex="0" GridPane.rowIndex="0" fx:id="checkBoxResults1"/>
                <CheckBox text="Zbrojenie poprzeczne" GridPane.columnIndex="1" GridPane.rowIndex="0" fx:id="checkBoxResults2"/>
                <CheckBox text="SGU" GridPane.columnIndex="2" GridPane.rowIndex="0" fx:id="checkBoxResults3"/>

                <Button text="Obliczenia" GridPane.columnIndex="1" GridPane.rowIndex="4"/>

            </GridPane>
        </GridPane>


    </center>

</BorderPane>

